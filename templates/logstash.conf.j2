input {
{% for k, v in logstash_docker_inputs.iteritems() %}
  {{ k }} {
{% for key, value in v.iteritems() %}
    {{ key }} => {{ value }}
{% endfor %}
  }
{% endfor %}
}

output {
{% for k, v in logstash_docker_outputs.iteritems() %}
  {{ k }} {
{% for key, value in v.iteritems() %}
    {{ key }} => {{ value }}
{% endfor %}
  }
{% endfor %}
}
